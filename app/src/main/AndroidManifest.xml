<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.oguzbabaoglu.kayakairlines"
          xmlns:android="http://schemas.android.com/apk/res/android"
    >

  <application
      android:allowBackup="true"
      android:icon="@mipmap/logo"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      >
    <activity
        android:name=".features.airlinelist.AirlineListActivity"
        android:label="@string/airline_list_title"
        android:theme="@style/AppTheme.NoActionBar"
        >
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".features.airlinedetail.AirlineDetailActivity">
    </activity>
  </application>

</manifest>